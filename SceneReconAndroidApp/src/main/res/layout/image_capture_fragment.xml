<?xml version="1.0" encoding="utf-8"?><FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"             xmlns:sceneRecon="http://schemas.android.com/apk/res-auto"             android:orientation="vertical"             android:layout_width="match_parent"             android:layout_height="match_parent">    <RelativeLayout            android:layout_width="match_parent"            android:layout_height="match_parent">        <SurfaceView                android:id="@+id/camera_surface_view"                android:layout_width="match_parent"                android:layout_height="match_parent"/>        <edu.cmu.navlab.scenerecon.view.customviews.CameraGridView                android:id="@+id/camera_grid_view"                android:layout_width="match_parent"                android:layout_height="match_parent"                sceneRecon:background_color="@color/clear"                sceneRecon:base_line_color="@color/gray"                sceneRecon:indicator_color_aligned="@color/emerald"                sceneRecon:indicator_color_unaligned="@color/alizarin"/>        <Button                android:id="@+id/take_picture_button"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/take_picture"                android:layout_alignParentTop="true"                android:layout_alignParentRight="true"                android:layout_alignParentEnd="true"                android:layout_marginTop="90dp"/>                <TextView            android:id="@+id/scene_recon_instruction_view"            android:text="@string/blank_instruction"            android:gravity="center"            android:textColor="#C0FFFFFF"            android:background="#60969696"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_alignParentBottom="true"/>    </RelativeLayout>    <FrameLayout            android:id="@+id/scene_recon_camera_progress_container"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:clickable="true">        <ProgressBar                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center"/>    </FrameLayout></FrameLayout>